		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
			<style>
				.btn-loope {
					margin-top: 8px !important;
					background-color: #FFF !important;
					border: 1px solid #cfdbe6 !important;
				}
				.dropdown-toggle {
					display: flex;
