{% extends 'backoffice/base.html.twig' %}

{% block title %}Depot{% endblock %}

{% block body %}
    <h1>Depot</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ depot.id }}</td>
            </tr>
            <tr>
                <th>Idenfiant</th>
                <td>{{ depot.idDossier }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ depot.dateDepot ? depot.dateDepot|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ depot.etatDossier }}</td>
            </tr>
            <tr>
                <th>Regime</th>
                <td>{{ depot.regime }}</td>
            </tr>
            <tr>
                <th>Total depense</th>
                <td>{{ depot.totalDepense }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_depot_index') }}">Retour</a>

    <a href="{{ path('app_depot_edit', {'id': depot.id}) }}">Modifier</a>

    {{ include('depot/_delete_form.html.twig') }}
{% endblock %}
